    <dct:title>Improving the Performance of Semantic Web Applications with SPARQL Query Caching</dct:title>
    <dct:issued>2010</dct:issued>
    <dct:creator>
      <rdf:Seq>
        <rdf:li>Michael Martin</rdf:li>
