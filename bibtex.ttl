    dct:issued "2016-05" ;
    dct:title "Towards Sustainable view-based Extract-Transform-Load (ETL) Fusion of Open Data" ;
    bibtex:keywords "kmueller stadler singh hellmann smart-data-web-project" ;
    bibtex:month "May" ;
    a bibtex:InProceedings .

