    dct:isPartOf "12th International Conference on Semantic Systems Proceedings (SEMANTiCS 2016)", "SEMANTiCS '16" ;
    dct:issued "2016-09" ;
    dct:title "Quit Diff: Calculating the Delta Between RDF Datasets under Version Control" ;
